{
    "name": "hotel_minimun_tourism_test",
    "fiware-service": "test",
    "fiware-servicepath": "/tourism",
    "subscriptions": {
        "sgtr": {
            "description": "kafka sgtr tourism hotel create",
            "status": "active",
            "subject": {
                "entities": [
                    {
                        "idPattern": ".*",
                        "type": "Hotel"
                    }
                ],
                "condition": {
                    "attrs": [
                        "TimeInstant"
                    ]
                },
                "alterationTypes": [
                    "entityCreate", "entityUpdate", "entityChange"
                ]
            },
            "notification": {
                "kafkaCustom": {
                    "url": "kafka://kafka:9092",
                    "topic": "raw_sgtr",
                    "json": {
                        "data": [
                            {
                                "alterationType": "${alterationType}",
                                "type": "${type}",
                                "externalId": "${name}",
                                "name": "${name}",
                                "registrationNumber": "${registrationNumber}",
                                "hasAdditionalType": "${hasAdditionalType}",
                                "autonomousCheckIn": "${autonomousCheckIn}",
                                "chainOrGroup": "${chainOrGroup}",
                                "checkInHours": "${checkInHours}",
                                "checkOutHours": "${checkOutHours}",
                                "numberOfBeds": "${numberOfBeds}",
                                "numberOfAccommodationUnits": "${numberOfAccommodationUnits}",
                                "parador": "${parador}",
                                "hasLocation": {
                                    "object": {
                                        "autonomousCommunity": "${hasLocation}",
                                        "country": "${hasLocation}",
                                        "locality": "${hasLocation}",
                                        "postalCode": "${hasLocation}",
                                        "province": "${hasLocation}",
                                        "streetAddress": "${hasLocation}",
                                        "asWkt": "POINT(12.4964 41.9028)",
                                        "lat": 12.4,
                                        "long": 41.9
                                    }
                                },
                                "hasDescription": {
                                    "object": {
                                        "shortDescription": "${hasDescription}",
                                        "longDescription": "${hasDescription}",
                                        "keyword": []
                                    }
                                }
                                
                            }
                        ]
                    }
                },
                "attrs": [
                    "TimeInstant"
                ]
            }
        }
    },
