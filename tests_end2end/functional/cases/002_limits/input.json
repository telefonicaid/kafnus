  "fiware-servicepath": "/limit",
  "subscriptions": {
    "historic": {
      "description": "Sensor:HISTORIC:limit:historic",
      "status": "active",
      "subject": {
        "entities": [
          {
            "idPattern": ".*",
            "type": "Sensor"
          }
        ],
        "condition": {
          "attrs": [
            "TimeInstant"
          ]
        }
      },
      "notification": {
        "mqttCustom": {
          "url": "mqtt://mosquitto:1883",
          "topic": "kafnus/${service}${servicePath}/raw_historic"
        },
        "attrs": [
          "TimeInstant",
          "value_real",
          "value_double",
          "value_numeric",
          "value_smallint",
          "value_integer",
          "value_bigint",
          "geom_point",
